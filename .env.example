PAPER_MODE=true
SYMBOL=BTC/USDT
TIMEFRAME=1m
LOOP_INTERVAL_SECONDS=60
LOOKBACK=250
STARTING_BALANCE=10000
RISK_PER_TRADE_PCT=0.5
